set terminal svg size 800,600
set output "plots/error.svg"
set style line 1 lt 1 lw 2
set style line 2 lt 2 lw 2
set style line 3 lt 3 lw 1 lc rgb "red"
set style line 4 lt 4 lw 1 lc rgb "blue"
set grid xtics ytics
set xlabel "Размер входного массива"
set ylabel "Время выполнения"
set boxwidth 0.5 relative
set style fill solid 0.3

plot for [i=1:words(system("ls plot_data/error/"))] "plot_data/error/".word(system("ls plot_data/error/"), i) using 1:2:3:4 with errorlines linestyle 1